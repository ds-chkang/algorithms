    "# 리스트를 이용한 Queue 재현\n",
    "\n",
    "class Queue:\n",
    "    def __init__(self):\n",
    "        self.queue = []\n",
    "    def push(self, data):\n",
    "        self.queue.append(data)\n",
    "    def pop(self):\n",
    "        del self.queue[0]\n",
    "    def show(self):\n",
    "        print (self.queue)"
