    "class Stack:\n",
    "    def __init__(self):\n",
    "        self.stack = []\n",
    "    def push(self, data):\n",
    "        self.stack.append(data)\n",
    "    def pop(self):\n",
    "        self.stack.pop()\n",
    "    def peek(self):\n",
    "        print (self.stack[-1])\n",
    "    def show(self):\n",
    "        print (self.stack)"
   ]
